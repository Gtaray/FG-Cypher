<root>
	<panel name="global_difficulty">
		<class>global_difficulty</class>
		<bounds>15,-90,32,32</bounds>
		<locked />
	</panel>

	<panel name="global_gmi_threshold">
		<class>global_gmi_threshold</class>
		<bounds>50,-70,25,25</bounds>
		<locked />
	</panel>

	<panel name="modifierstack">
		<class>modifierstack</class>
		<bounds>75,-100,64,57</bounds>
		<dynamic />
		<locked />
	</panel>

	<panel name="difficultyadjustpanel">
		<class>difficultyadjust</class>
		<bounds>150,-95,210,52</bounds>
		<dynamic />
		<locked />
	</panel>

	<panel name="dicepanel" merge="join">
		<anchored>
			<left offset="360" />
		</anchored>
	</panel>
</root>